<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Hibernate-mapping DTD 3.0//EN"
     "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="cn.com.sway.cms.config.model.Config">
		<id name="id">
			<generator class="assigned" />
		</id>
		<property name="remark" not-null="true"/>
		<property name="allowDelete" not-null="true"/>
		<property name="orderNum" not-null="true"/>
		<property name="type" not-null="true" />
		<property name="maxLength" not-null="true"/>
		<property name="value" type="text" not-null="true"/>
	</class>

</hibernate-mapping>